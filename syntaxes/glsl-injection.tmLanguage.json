{
  "fileTypes": [],
  "injectionSelector": "L:source.ts,L:source.js",
  "patterns": [
    {
      "include": "#glsl-injection"
    }
  ],
  "repository": {
    "glsl-injection": {
      "patterns": [
        {
          "begin": "(vertexShader|fragmentShader|glsl)\\s*=\\s*`",
          "end": "`",
          "contentName": "source.glsl",
          "patterns": [
            { "include": "source.glsl" }
          ]
        }
      ]
    }
  },
  "scopeName": "source.ts"
}
